				</textElement>
				<text><![CDATA[FECHA DE SUPSCRIPCIÓN]]></text>
			</staticText>
			<staticText>
				<reportElement x="-10" y="828" width="589" height="378" uuid="732a6f45-303d-4ee4-b988-f5099a749696">
